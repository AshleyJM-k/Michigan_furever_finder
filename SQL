
CREATE TABLE BREED(
breed_id int,
breed tex
);

CREATE TABLE PERSONALITY(
personality_id int,
personality text
);

CREATE TABLE DOG_COLOR (
colorId text(15)
);

CREATE TABLE DOG_BREED (
breedID int,
multi boolean
);

CREATE TABLE DOG (
dogID int,
name text(15),
age int(2),
sex text(6),
birthday date,
picture text,
status text(15), 
records text(15),
);

CREATE TABLE Persons (
    PersonID int,
    LastName varchar(255),
    FirstName varchar(255),
    Address varchar(255),
    City varchar(255)
);
